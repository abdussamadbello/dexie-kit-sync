{
  "name": "@dexie-kit/sync",
  "version": "1.0.0",
  "description": "Offline-first sync framework for Dexie.js with REST API support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "keywords": [
    "dexie",
    "sync",
    "offline-first",
    "indexeddb",
    "rest-api",
    "pwa",
    "offline",
    "synchronization"
  ],
  "author": "Abdussamad Bello",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/abdussamadbello/dexie-sync-kit.git"
  },
  "bugs": {
    "url": "https://github.com/abdussamadbello/dexie-sync-kit/issues"
  },
  "homepage": "https://github.com/abdussamadbello/dexie-sync-kit#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "dexie": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "dexie": "^4.0.1",
    "eslint": "^8.56.0",
    "fake-indexeddb": "^5.0.2",
    "jsdom": "^27.0.1",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
